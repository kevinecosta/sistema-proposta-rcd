# Configurações do Supabase
VITE_SUPABASE_URL=sua_url_do_supabase
VITE_SUPABASE_ANON_KEY=sua_chave_anonima_do_supabase

# Configurações do Ambiente
NODE_ENV=development # ou production

# Configurações da Aplicação
VITE_APP_NAME="Sistema Rede Contábil Digital"
VITE_APP_URL=http://localhost:5173 # URL base da aplicação

# Configurações de Upload
VITE_MAX_UPLOAD_SIZE=5242880 # Tamanho máximo de upload em bytes (5MB)

# Configurações de Cache
VITE_CACHE_TTL=3600 # Tempo de vida do cache em segundos

# Configurações de Segurança
VITE_API_TIMEOUT=30000 # Timeout das requisições em milissegundos 